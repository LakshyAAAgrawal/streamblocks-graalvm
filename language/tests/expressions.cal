namespace test:

	actor test() ==> :
		action ==>
		var
			f := lambda (/*int*/ x) var /*int*/ y := 2: y - x end,
			/*int*/ a := 1
		do
            a := a ^ a;
			a := a + 2 - 1;
			a := -a + 2;
			a := a * 2 / 2;
			a := if (a % 3 != 1) then println("Error!") else a end;
			a := a * 2 >> 1;
			a := (a << 1) / 2;
			a := if (a == 1) then if (a >= 0) then if (a < 2) then a else 0 end else 0 end else 0 end;
            a := let b := a + 1: b - a end;
            a := f(a);
			println(a);
		end
	end
end
